{
  "name": "newamerica-cms",
  "version": "1.0.0",
  "description": "Custom Content Management System (CMS) built for New America",
  "main": "index.js",
  "engines": {
    "node": "6.9.1",
    "npm": "5.4.2"
  },
  "scripts": {
    "start": "source .env & ./manage.py runserver",
    "open": "open https://localhost:8000/",
    "dev": "npm run watch:js & npm run watch:css",
    "watch:js": "NODE_ENV=development; rollup -c rollup.dev.js --watch",
    "build:js": "rollup -c rollup.dev.js",
    "build:standalone-js": "NODE_ENV=production rollup -c rollup.standalone.js",
    "watch:css": "watch 'npm run build:css' newamericadotorg/assets/scss --interval=1",
    "build:css": "node-sass --include-path='node_modules' --include-path=newamericadotorg/assets/scss/settings/development --include-path='newamericadotorg/assets/scss' newamericadotorg/assets/scss/all.scss newamericadotorg/static/css/newamericadotorg.css",
    "build:production": "NODE_ENV=production rollup -c",
    "heroku-postbuild": "npm run build:production && npm run build:standalone-js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newamericafoundation/newamerica-cms.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/newamericafoundation/newamerica-cms/issues"
  },
  "homepage": "https://github.com/newamericafoundation/newamerica-cms#readme",
  "dependencies": {
    "date-fns": "^1.28.5",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.6.0",
    "react-dates": "^12.6.0",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-slick": "^0.14.11",
    "react-transition-group": "^1.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "url-polyfill": "^1.0.5",
    "vanilla-lazyload": "^7.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-react": "^6.16.0",
    "cssnano": "^3.10.0",
    "eslint": "^2.9.0",
    "eslint-plugin-babel": "^3.2.0",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-sass": "^0.4.8",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^3.2.2",
    "watch": "^1.0.2"
  }
}
