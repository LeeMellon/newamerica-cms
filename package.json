{
  "name": "newamerica-cms",
  "version": "1.0.0",
  "description": "Custom Content Management System (CMS) built for New America",
  "main": "index.js",
  "engines": {
    "node": "6.9.1"
  },
  "scripts": {
    "start": "source .env & ./manage.py runserver",
    "open": "open https://localhost:8000/",
    "dev": "npm run watch:js & npm run watch:css",
    "watch:js": "NODE_ENV=development; rollup -c rollup.dev.js --watch",
    "watch:css": "NODE_ENV=development; node-sass newamericadotorg/assets/scss/newamericadotorg.scss --output='newamericadotorg/static/css/' --include-path='node_modules' --include-path=\"newamericadotorg/assets/scss/settings/$NODE_ENV\" --include-path='newamericadotorg/assets/scss' --watch=newamericadotorg/assets/scss",
    "heroku-postbuild": "npm run build",
    "build": "NODE_ENV=production rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newamericafoundation/newamerica-cms.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/newamericafoundation/newamerica-cms/issues"
  },
  "homepage": "https://github.com/newamericafoundation/newamerica-cms#readme",
  "dependencies": {
    "whatwg-fetch": "^2.0.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "prop-types": "^15.5.10",
    "redux": "^3.6.0",
    "react-redux": "^4.4.5",
    "redux-thunk": "^2.2.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "date-fns": "^1.28.5",
    "vanilla-lazyload": "^7.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^2.9.0",
    "eslint-plugin-babel": "^3.2.0",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.5.3",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-sass": "^0.4.8",
    "rollup-watch": "^3.2.2",
    "watch": "^1.0.2"
  }
}
